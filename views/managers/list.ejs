<%- include('header'); -%>
<h1><%= title %></h1>
<p>Welcome to <%= title %></p>


<table>
    <thead>
        <th>ID#</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Username</th>
        <th>Role</th>
        <th>Address</th>
        <th>Birthdate</th>
        <th>Hire Date</th>
    </thead>
    <tbody>
        <% users.forEach(function(user) { %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.firstName %></td>
            <td><%= user.lastName %></td>
            <td><%= user.userName %></td>
            <td><%= user.role %></td>
            <td><%= user.address %></td>
            <td><%= user.birthDate %></td>
            <td><%= user.hireDate %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
